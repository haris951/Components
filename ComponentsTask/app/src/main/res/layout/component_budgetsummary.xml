<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="170dp"
    android:layout_height="80dp"
    android:layout_marginTop="5dp"
    android:background="@drawable/budget_summary_rect">
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/ivGroup222"
        android:src="@drawable/group_222__1_"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        style="@style/BudgetImageView" />
    <TextView
        android:id="@+id/textViewTitle"
        android:text="@string/title"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.183"
        app:layout_constraintStart_toEndOf="@+id/ivGroup222"
        app:layout_constraintTop_toTopOf="parent"
        style="@style/BudgetTextView1" />
    <TextView
        android:id="@+id/tvAmount"
        android:text="@string/amount"
        app:layout_constraintStart_toEndOf="@id/ivGroup222"
        app:layout_constraintTop_toBottomOf="@+id/textViewTitle"
        style="@style/BudgetTextView2" />

</androidx.constraintlayout.widget.ConstraintLayout>